#!/bin/bash

URL=$1

function open_url() {
  BROWSER=$1
  open "$URL" -a "$BROWSER"
}

SAFARI_PATTERNS=(
  '^https\?://mail\.alibaba-inc\.com'
  '^https\?://[^/]*127\.0\.0\.1:[0-9]*/Dash/'
)

CHROME_PATTERNS=(
'^https\?://www\.umeng\.com'
'^https\?://github\.umeng\.co'
'^https\?://localhost'
'^https\?://[^/]*\.umeng\.us'
'^https\?://[^/]*docs\.google\.com'
'^https\?://[^/]*\.alibaba-inc\.com'
'^https\?://[^/]*\.quip\.com'
'^https\?://[^/]*\.ktjr\.com'
'^https\?://[^/]*\.kaitongamc\.com'
'^https\?://[^/]*\.creditcloud\.com'
'^https\?://[^/]*\.duoduohaoche\.com'
'^https\?://[^/]*\.huashenghaoche\.com'
'^https\?://[^/]*\.maodou\.com'
'^https\?://[^/]*\.tangeche\.com'
'^https\?://[^/]*\.mockplus\.cn'
'^https\?://[^/]*localhost'
'^https\?://[^/]*127\.0\.0\.1'
'^http://123.56.85.83/'
'^http://123.57.61.34/'
'^https://.*.axshare.com/'
'^https://lanhuapp.com/'
'tiaoyin100'
)

FIREFOX_PATTERNS=()

if echo "$URL" | grep -q '^https\?://mailsso.mxhichina.com'; then
    open "https://webmail.alibaba-inc.com/alimail/" -a "Google Chrome"
    exit
fi

for pa in "${SAFARI_PATTERNS[@]}"
do
  if echo "$URL" | grep -q "$pa"
  then
    open_url "Safari"
    exit
  fi
done

for pa in "${CHROME_PATTERNS[@]}"
do
  if echo "$URL" | grep -q "$pa"
  then
    open_url "Google Chrome"
    exit
  fi
done

for pa in "${FIREFOX_PATTERNS[@]}"
do
  if echo "$URL" | grep -q "$pa"
  then
    open_url "Firefox"
    exit
  fi
done



open_url "Safari"

