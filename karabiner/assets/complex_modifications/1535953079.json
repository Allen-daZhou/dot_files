{
  "title": "Open Directories",
  "rules": [
    {
      "description": "Open Directories",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "f_mode",
                "value": 1
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "f_mode",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open $HOME'/Downloads'"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "f_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
